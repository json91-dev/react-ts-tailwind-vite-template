{
  "singleQuote": true,
  "trailingComma": "all",
  "semi": false,
  "useTabs": false,
  "endOfLine": "auto",
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "printWidth": 120,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 200
      }
    }
  ]
}
